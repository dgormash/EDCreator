<Window x:Class="EDCreator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EDCreator"
        mc:Ignorable="d"
        Title="MainWindow" Height="260" Width="630">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top" HorizontalAlignment="Stretch" Margin="10 0 0 0">
            <Button Content="Login" MinWidth="100" Click="Button_Click"/>
            <Button Content="Workshop"  MinWidth="100" Margin="10 0 0 0" Click="Button_Click_1"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35, 0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
